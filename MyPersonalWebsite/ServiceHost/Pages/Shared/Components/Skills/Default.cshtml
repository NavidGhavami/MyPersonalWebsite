@model List<_01_Query.Contract.Skills.SkillQueryModel>


<div class="lonon-skills">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <span class="heading-meta style-1">مهارت ها</span>
                <h2 class="lonon-heading animate-box" data-animate-effect="fadeInLeft">مهارت و توانایی های من</h2>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12 animate-box" data-animate-effect="fadeInLeft">
                @foreach (var item in Model)
                {
                    <p class="bar-title">@item.SkillName<span class="percent align-right">@item.SkillPercent%</span></p>
                    <div class="bar">
                        <div class="bar-fill" style="width: @item.SkillPercent%; background-color: black;"></div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>